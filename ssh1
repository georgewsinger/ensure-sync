[38;5;130m 13 
 14 [m[34m##SSH1=""[m
[38;5;130m 15 [m[34m#SSH1="ssh -p 58002 george@192.168.1.2"[m
[38;5;130m 16 
 17 [m[34m##SSH2=""[m
[38;5;130m 18 [m[34m#SSH2="ssh -p 58005 george@192.168.1.5"[m
[38;5;130m 19 [m[34m#DIR1="/home/george/Dropbox"[m
[38;5;130m 20 [m[34m#DIR2="/home/george/Dropbox"[m
[38;5;130m 21 
 22 [m[36mLS1[m=[38;5;130m"[m[31m./ls1[m[38;5;130m"
 23 [m[36mLS2[m=[38;5;130m"[m[31m./ls2[m[38;5;130m"
 24 
 25 echo[m[31m [m[38;5;130m"[m[31mGathering list of files to compare (this may take a few seconds).[m[38;5;130m"
