 21 if [[m [38;5;130m"[m[35m$SSH2[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 22 [m  [34m#ls -1a -d -1 $DIR2/{*,.*} > $LS1[m
[38;5;130m 23 [m  [38;5;130mls[m [35m-1a[m [38;5;130m-d[m [31m-1[m [35m$DIR2[m/[35m{[m*,.*[35m}[m [38;5;130m>[m [35m$LS1[m
[38;5;130m 24 else
 25 [m  [35m$SSH2[m [38;5;130m"[m[31mls -1a -d -1 [m[35m$DIR2[m[31m/{*,.*}[m[38;5;130m"[m [38;5;130m>[m [35m$LS2[m
[38;5;130m 26 fi
 27 
 28 [m[34m# Correct for strange non-sorting behavior in SSH[m
[38;5;130m 29 [mcat [35m$LS1[m | [38;5;130msort[m [35m-o[m [35m$LS1[m
[38;5;130m 30 [mcat [35m$LS2[m | [38;5;130msort[m [35m-o[m [35m$LS2[m
[38;5;130m 31 
 32 [mawk [38;5;130m'[m[31m{[m
[38;5;130m 33 [m[31m    print[m
[38;5;130m 34 [m[31m    getline line < second[m
[38;5;130m 35 [m[31m    if ($0 != line) print line[m
[38;5;130m 36 [m[31m}[m[38;5;130m'[m [36msecond[m=[35m$LS2[m [35m$LS1[m [38;5;130m>[m [38;5;130m'[m[31m./ls3[m[38;5;130m'
 37 
 38 [m[34m# Delete first two lines of each file (noise)[m
[38;5;130m 39 sed[m [35m-i[m [35m-e[m [31m1[m,2d [35m$LS1[m
[38;5;130m 40 sed[m [35m-i[m [35m-e[m [31m1[m,2d [35m$LS2[m
[38;5;130m 41 
 42 [m[36mfunction[m [36mcheck_input_dir_file_for_hash_consistencies() {[m
[38;5;130m 43 
 44 [m  [38;5;130mwhile read line
 45   do
 46 [m
[1m[7mensure-sync.sh                                                                                                                                                              1,1            Top]2;ensure-sync.sh (~/Dropbox/ensure-sync) - VIM]1;ensure-sync.sh[1;5H[?12l[?25hP+q436f\P+q6b75\P+q6b64\P+q6b72\P+q6b6c\P+q2332\P+q2334\P+q2569\P+q2a37\P+q6b31\[?25l[1;47r[m[1;1H[28M[1;49r[1;2H[38;5;130m46
