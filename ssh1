[38;5;130m 26 [m[34m###"$SSH1" "ls -1a -d -1 $DROPBOX/{*,.*} > $DROPBOX/hash/ArchBox_dirs"[m
[38;5;130m 27 
 28 [m[34m#$SSH1 "ls -1a -d -1 $DIR1/{*,.*}" > $LS1[m
[38;5;130m 29 [m[34m#$SSH2 "ls -1a -d -1 $DIR2/{*,.*}" > $LS2[m
[38;5;130m 30 if [[m [38;5;130m"[m[35m$SSH1[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
