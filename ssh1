 27 [m[34m# ls -1a -d -1 $PWD/{*,.*}[m
[38;5;130m 28 [m[34m###"$SSH1" "ls -1a -d -1 $DROPBOX/{*,.*} > $DROPBOX/hash/ArchBox_dirs"[m
[38;5;130m 29 
 30 [m[34m#$SSH1 "ls -1a -d -1 $DIR1/{*,.*}" > $LS1[m
[38;5;130m 31 [m[34m#$SSH2 "ls -1a -d -1 $DIR2/{*,.*}" > $LS2[m
[38;5;130m 32 if [[m [38;5;130m"[m[35m$SSH1[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 33 [m  [34m#ls -1a -d -1 $DIR1/{*,.*} > $LS1[m
[38;5;130m 34 [m  [38;5;130mls[m [35m-1a[m [38;5;130m-d[m [31m-1[m [35m$(realpath $DIR1)[m/[35m{[m*,.*[35m}[m [38;5;130m>[m [35m$LS1[m
[38;5;130m 35 else
 36 [m  [35m$SSH1[m [38;5;130m"[m[31mls -1a -d -1 [m[35m$(realpath $DIR1)[m[31m/{*,.*}[m[38;5;130m"[m [38;5;130m>[m [35m$LS1[m
[38;5;130m 37 fi
 38 
 39 if [[m [38;5;130m"[m[35m$SSH2[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 40 [m  [34m#ls -1a -d -1 $DIR2/{*,.*} > $LS1[m
[38;5;130m 41 [m  [38;5;130mls[m [35m-1a[m [38;5;130m-d[m [31m-1[m [35m$DIR2[m/[35m{[m*,.*[35m}[m [38;5;130m>[m [35m$LS1[m
[38;5;130m 42 else
 43 [m  [35m$SSH2[m [38;5;130m"[m[31mls -1a -d -1 [m[35m$DIR2[m[31m/{*,.*}[m[38;5;130m"[m [38;5;130m>[m [35m$LS2[m
[38;5;130m 44 fi
 45 
 46 [m[34m# Correct for strange non-sorting behavior in SSH[m
[1m[7mensure-sync.sh                                                                                                                                                              1,1            Top[1;5H[?12l[?25h[?25l[27m[23m[m[H[2J[1;1H[38;5;130m 46 [m[36mfunction[m [36mcheck_input_dir_file_for_hash_consistencies() {[m
[38;5;130m 45 
 44 [m  [38;5;130mwhile read line
 43   do
 42 [m    [34m#hashThroughSSH1="$($SSH1 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 41 [m    [34m#hashThroughSSH2="$($SSH2 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 40 
 39 [m    [38;5;130mif [[m [38;5;130m"[m[35m$SSH1[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 38 [m      [36mhashThroughSSH1[m=[35m$([m[38;5;130mfind[m[35m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[35m -type f -print0 [m[38;5;130m|[m[35m [m[38;5;130msort[m[35m -z [m[38;5;130m|[m[35m xargs [m[31m-0[m[35m sha1sum [m[38;5;130m|[m[35m sha1sum)[m
[38;5;130m 37 [m    [38;5;130melse
 36 [m      [36mhashThroughSSH1[m=[38;5;130m"[m[35m$($SSH1 [m[38;5;130m"[m[31mfind [m[35m\"$line\"[m[31m -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum[m[38;5;130m"[m[35m < /dev/null)[m[38;5;130m"
 35 [m    [38;5;130mfi
 34 
 33 [m    [38;5;130mif [[m [38;5;130m"[m[35m$SSH2[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 32 [m      [36mhashThroughSSH2[m=[35m$([m[38;5;130mfind[m[35m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[35m -type f -print0 [m[38;5;130m|[m[35m [m[38;5;130msort[m[35m -z [m[38;5;130m|[m[35m xargs [m[31m-0[m[35m sha1sum [m[38;5;130m|[m[35m sha1sum)[m
[38;5;130m 31 [m    [38;5;130melse
 30 [m      [36mhashThroughSSH2[m=[38;5;130m"[m[35m$($SSH2 [m[38;5;130m"[m[31mfind [m[35m\"$line\"[m[31m -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum[m[38;5;130m"[m[35m < /dev/null)[m[38;5;130m"
 29 [m    [38;5;130mfi
 28 
 27 [m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$hashThroughSSH1[m[38;5;130m"
 26 [m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$hashThroughSSH2[m[38;5;130m"
 25 
 24 [m    [38;5;130mif [[m [38;5;130m"[m[35m$hashThroughSSH1[m[38;5;130m"[m [38;5;130m==[m [38;5;130m"[m[35m$hashThroughSSH2[m[38;5;130m"[m [38;5;130m]
 23 [m    [38;5;130mthen
 22 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is synced[m[38;5;130m"
 21 [m      [38;5;130mcontinue[m [34m# safeguard against my own stupidity[m
[38;5;130m 20 [m    [38;5;130melse
 19 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is not synced[m[38;5;130m"[m [34m# safeguard against my own stupidity[m
[38;5;130m 18 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[31m [m[38;5;130m>>[m [38;5;130m"[m[31m./not-synced[m[38;5;130m"
 17 [m      [34m# break[m
[38;5;130m 16 [m    [38;5;130mfi
 15 [m  [38;5;130mdone
 14 
 13 [m[36m}[m
[38;5;130m 12 
 11 [m[36mDIFF[m=[35m$(diff [m[38;5;130m"[m[35m$LS1[m[38;5;130m"[m[35m [m[38;5;130m"[m[35m$LS2[m[38;5;130m"[m[35m)[m
[38;5;130m 10 [m[34m#LS3=$(cat $LS1 $LS2 | sort | uniq)[m
[38;5;130m  9 [m[36mLS3[m=[35m$LS1[m
[38;5;130m  8 
  7 if [[m [38;5;130m"[m[35m$DIFF[m[38;5;130m"[m [38;5;130m==[m [38;5;130m""[m [38;5;130m]
  6 then
  5 [m  cat [35m$LS3[m [38;5;130m|[m check_input_dir_file_for_hash_consistencies
[38;5;130m  4 else
  3 [m  [34m# echo "LS1 differs from LS2."[m
[38;5;130m  2 [m  cat [35m$LS3[m [38;5;130m|[m check_input_dir_file_for_hash_consistencies
[38;5;130m  1 [m  [34m#echo "LS1 differs from LS2."[m
[38;5;130m101 fi[m
