 23 [m    [38;5;130mthen
 22 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is synced[m[38;5;130m"
 21 [m      [38;5;130mcontinue[m [34m# safeguard against my own stupidity[m
[38;5;130m 20 [m    [38;5;130melse
 19 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is not synced[m[38;5;130m"[m [34m# safeguard against my own stupidity[m
[38;5;130m 18 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[31m [m[38;5;130m>>[m [38;5;130m"[m[31m./not-synced[m[38;5;130m"
 17 [m      [34m# break[m
[38;5;130m 16 [m    [38;5;130mfi
 15 [m  [38;5;130mdone
 14 
 13 [m[36m}[m
[38;5;130m 12 
