 33 [m    [38;5;130mif [[m [38;5;130m"[m[35m$SSH2[m[38;5;130m"[m [38;5;130m=[m [31m"local"[m [38;5;130m];[m [38;5;130mthen
 32 [m      [36mhashThroughSSH2[m=[35m$([m[38;5;130mfind[m[35m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[35m -type f -print0 [m[38;5;130m|[m[35m [m[38;5;130msort[m[35m -z [m[38;5;130m|[m[35m xargs [m[31m-0[m[35m sha1sum [m[38;5;130m|[m[35m sha1sum)[m
[38;5;130m 31 [m    [38;5;130melse
 30 [m      [36mhashThroughSSH2[m=[38;5;130m"[m[35m$($SSH2 [m[38;5;130m"[m[31mfind [m[35m\"$line\"[m[31m -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum[m[38;5;130m"[m[35m < /dev/null)[m[38;5;130m"
 29 [m    [38;5;130mfi
 28 
 27 [m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$hashThroughSSH1[m[38;5;130m"
 26 [m    [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$hashThroughSSH2[m[38;5;130m"
 25 
 24 [m    [38;5;130mif [[m [38;5;130m"[m[35m$hashThroughSSH1[m[38;5;130m"[m [38;5;130m==[m [38;5;130m"[m[35m$hashThroughSSH2[m[38;5;130m"[m [38;5;130m]
 23 [m    [38;5;130mthen
 22 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is synced[m[38;5;130m"
 21 [m      [38;5;130mcontinue[m [34m# safeguard against my own stupidity[m
[38;5;130m 20 [m    [38;5;130melse
 19 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is not synced[m[38;5;130m"[m [34m# safeguard against my own stupidity[m
[38;5;130m 18 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[31m [m[38;5;130m>[m [38;5;130m"[m[31m./not-synced[m[38;5;130m"
 17 [m      [34m# break[m
[38;5;130m 16 [m    [38;5;130mfi
 15 [m  [38;5;130mdone
 14 
 13 [m[36m}[m
[38;5;130m 12 
 11 [m[36mDIFF[m=[35m$(diff [m[38;5;130m"[m[35m$LS1[m[38;5;130m"[m[35m [m[38;5;130m"[m[35m$LS2[m[38;5;130m"[m[35m)[m
[38;5;130m 10 [m[34m#LS3=$(cat $LS1 $LS2 | sort | uniq)[m
[38;5;130m  9 [m[36mLS3[m=[35m$LS1[m
[38;5;130m  8 
  7 if [[m [38;5;130m"[m[35m$DIFF[m[38;5;130m"[m [38;5;130m==[m [38;5;130m""[m [38;5;130m]
  6 then
  5 [m  cat [35m$LS3[m [38;5;130m|[m check_input_dir_file_for_hash_consistencies
[38;5;130m  4 else
  3 [m  [34m# echo "LS1 differs from LS2."[m
[38;5;130m  2 [m  cat [35m$LS3[m [38;5;130m|[m check_input_dir_file_for_hash_consistencies
[38;5;130m  1 [m  [34m#echo "LS1 differs from LS2."[m
[38;5;130m99  fi[m
