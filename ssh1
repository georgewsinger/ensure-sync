[38;5;130m 10 [m[36mLS3[m=[35m$(cat $LS1 $LS2 [m[38;5;130m|[m[35m [m[38;5;130msort[m[35m [m[38;5;130m|[m[35m uniq)[m
[38;5;130m  9 [m[34m#LS3=$LS1[m
[38;5;130m  8 
  7 if [[m [38;5;130m"[m[35m$DIFF[m[38;5;130m"[m [38;5;130m==[m [38;5;130m""[m [38;5;130m]
  6 then
