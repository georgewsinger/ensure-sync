[38;5;130m 45 [m[34m[47m#[m[34m function check_input_dir_file_for_hash_consistencies() {[m
[38;5;130m 44 [m[34m[47m#[m[34m [m
[38;5;130m 43 [m[34m[47m#[m[34m   while read line[m
[38;5;130m 42 [m[34m[47m#[m[34m   do[m
[38;5;130m 41 [m[34m[47m#[m[34m     #hashThroughSSH1="$($SSH1 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 40 [m[34m[47m#[m[34m     #hashThroughSSH2="$($SSH2 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 39 [m[34m[47m#[m[34m [m
[38;5;130m 38 [m[34m[47m#[m[34m     if [ "$SSH1" = "local" ]; then[m
[38;5;130m 37 [m[34m[47m#[m[34m       hashThroughSSH1=$(find "$line" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum)[m
[38;5;130m 36 [m[34m[47m#[m[34m     else[m
[38;5;130m 35 [m[34m[47m#[m[34m       hashThroughSSH1="$($SSH1 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 34 [m[34m[47m#[m[34m     fi[m
[38;5;130m 33 [m[34m[47m#[m[34m [m
[38;5;130m 32 [m[34m[47m#[m[34m     if [ "$SSH2" = "local" ]; then[m
[38;5;130m 31 [m[34m[47m#[m[34m       hashThroughSSH2=$(find "$line" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum)[m
[38;5;130m 30 [m[34m[47m#[m[34m     else[m
[38;5;130m 29 [m[34m[47m#[m[34m       hashThroughSSH2="$($SSH2 "find \"$line\" -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum" < /dev/null)"[m
[38;5;130m 28 [m[34m[47m#[m[34m     fi[m
[38;5;130m 27 [m[34m[47m#[m[34m [m
[38;5;130m 26 [m[34m[47m#[m[34m     echo "$hashThroughSSH1"[m
[38;5;130m 25 [m[34m[47m#[m[34m     echo "$hashThroughSSH2"[m
[38;5;130m 24 [m[34m[47m#[m[34m     [m
[38;5;130m 23 [m[34m[47m#[m[34m     if [ "$hashThroughSSH1" == "$hashThroughSSH2" ][m
[38;5;130m 22 [m[34m[47m#[m[34m     then[m
[38;5;130m 21 [m[34m[47m#[m[34m       echo "$line is synced"[m
[38;5;130m 20 [m[34m[47m#[m[34m       continue # safeguard against my own stupidity[m
[38;5;130m 19 [m[34m[47m#[m[34m     else[m
[38;5;130m 18 [m[34m[47m#[m[34m       echo "$line is not synced" # safeguard against my own stupidity[m
[38;5;130m 17 [m[34m[47m#[m[34m       echo "$line" > "./not-synced"[m
[38;5;130m 16 [m[34m[47m#[m[34m       # break[m
[38;5;130m 15 [m[34m[47m#[m[34m     fi[m
[38;5;130m 14 [m[34m[47m#[m[34m   done[m
[38;5;130m 13 [m[34m[47m#[m[34m [m
[38;5;130m 12 [m[34m[47m#[m[34m }[m
[38;5;130m 11 [m[34m[47m#[m[34m [m
[38;5;130m 10 [m[34m[47m#[m[34m DIFF=$(diff "$LS1" "$LS2")[m
[38;5;130m  9 [m[34m[47m#[m[34m LS3=$(cat $LS1 $LS2 | sort | uniq)[m
[38;5;130m  8 [m[34m[47m#[m[34m [m
[38;5;130m  7 [m[34m[47m#[m[34m if [ "$DIFF" == "" ] [m
[38;5;130m  6 [m[34m[47m#[m[34m then[m
[38;5;130m  5 [m[34m[47m#[m[34m   cat $LS3 | check_input_dir_file_for_hash_consistencies[m
[38;5;130m  4 [m[34m[47m#[m[34m else[m
[38;5;130m  3 [m[34m[47m#[m[34m   # echo "LS1 differs from LS2."[m
[38;5;130m  2 [m[34m[47m#[m[34m   cat $LS3 | check_input_dir_file_for_hash_consistencies[m
