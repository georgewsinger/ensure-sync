 11 [m    [38;5;130mthen
 10 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is synced[m[38;5;130m"
  9 [m      [38;5;130mcontinue
  8 [m    [38;5;130melse
  7 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[31m is not synced[m[38;5;130m"[m [34m# safeguard against my own stupidity[m
[38;5;130m  6 [m      [38;5;130mecho[m[31m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[31m [m[38;5;130m>>[m [38;5;130m"[m[31m./not-synced[m[38;5;130m"
