 20 [m      [36mhashThroughSSH2[m=[35m$([m[38;5;130mfind[m[35m [m[38;5;130m"[m[35m$line[m[38;5;130m"[m[35m -type f -print0 [m[38;5;130m|[m[35m [m[38;5;130msort[m[35m -z [m[38;5;130m|[m[35m xargs [m[31m-0[m[35m sha1sum [m[38;5;130m|[m[35m sha1sum)[m
[38;5;130m 19 [m    [38;5;130melse
 18 [m      [36mhashThroughSSH2[m=[38;5;130m"[m[35m$($SSH2 [m[38;5;130m"[m[31mfind [m[35m\"$line\"[m[31m -type f -print0 | sort -z | xargs -0 sha1sum | sha1sum[m[38;5;130m"[m[35m < /dev/null)[m[38;5;130m"
